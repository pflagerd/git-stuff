#!/bin/bash
declare -i i=1
remotes=""
for word in `git remote -v`; do
  if [ $i == 1 ]; then
    remotes+=$word" "
  fi
  if [ $i == 3 ]; then
    i=1
  else
    i=i+1
  fi
done

for remote in `echo "$remotes" | xargs -n1 | sort -u`; do
  echo "pushing $remote "`git remote get-url $remote`
  git push $remote
done
